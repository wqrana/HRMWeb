@model IEnumerable<TimeAide.Web.Models.Credential>

@Html.Partial("_IndexPageHeaderView")
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CredentialName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CredentialDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SelfServiceDisplay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SelfServiceUpload)
        </th>
        <th></th>
    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CredentialName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CredentialDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SelfServiceDisplay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SelfServiceUpload)
        </td>
        <td>
            @Html.Partial("_IndexPageButtonsView", item)
        </td>
    </tr>
    }

</tbody>
@Html.Partial("_IndexPageFooterView")
