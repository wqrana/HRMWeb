@model IEnumerable<TimeAide.Web.Models.IncidentType>
@Html.Partial("_IndexPageHeaderView")

<thead>
    <tr>
        <th>Action</th>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IncidentTypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IncidentTypeDescription)
        </th>
        
    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
    <tr>
        <td width="7%">
            @Html.Partial("_IndexPageButtonsView", item)
        </td>
        <td>
            @* @Html.DisplayFor(modelItem => item.Id)*@
            <a href="javascript:void(0);" id="anchorDetail" data-id="@item.Id">@item.Id</a>

        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IncidentTypeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IncidentTypeDescription)
        </td>
      

    </tr>
    }
</tbody>
    @Html.Partial("_IndexPageFooterView")

