@model IEnumerable<TimeAide.Web.Models.MaritalStatus>

@Html.Partial("_IndexPageHeaderView")
<thead>
    <tr>
        <th>
            Marital Status
        </th>
        <th>
            Description
        </th>
        <th></th>
    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MaritalStatusName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaritalStatusDescription)
            </td>
            <td>
                @Html.Partial("_IndexPageButtonsView", item)
            </td>
        </tr>
    }
</tbody>
@Html.Partial("_IndexPageFooterView")
