@model IEnumerable<TimeAide.Web.Models.EmployeeDocument>

<div id="DocumentUpoadHistory_modal" class="modal custom-modal">
    <div class="modal-dialog modal-dialog-centered modal-lg">
        <div class="modal-content">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Document upoad History for @ViewBag.DocumentTitle</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="table-responsive">
                                <table class="table  table-striped custom-table table-nowrap mb-0" style="width:100% !important">
                                    <thead>
                                        <tr>


                                            <th>Document Name</th>
                                            <th>Upload Date</th>
                                            <th>Download</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var eachEmployeeDocument in Model)
                                        {
                                        <tr>

                                            <td>@eachEmployeeDocument.DocumentName</td>
                                            <td>@eachEmployeeDocument.CreatedDate</td>
                                            <td><a href="javascript:downloadDocument(@eachEmployeeDocument.Id)" class="dropdown-item"><i class="fa fa-download m-r-5"></i> Download Document</a></td>
                                        </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="submit-section mt-1">

                        <button id="btnCancel" class="btn btn-primary  submit-btn" data-dismiss="modal" aria-label="Close">Closed</button>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
