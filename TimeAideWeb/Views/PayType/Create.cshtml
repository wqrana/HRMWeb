@model TimeAide.Web.Models.PayType

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal" style="padding:10px">
        @Html.AntiForgeryToken()
        @Html.Partial("_CreatePageTitleView")
        <div class="form-group row">
            <div class="col-md-2">
                @Html.LabelFor(model => model.PayTypeName, htmlAttributes: new { @class = "control-label" })
            </div>
            <div class="col-md-8">
                @Html.EditorFor(model => model.PayTypeName, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.PayTypeName, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="form-group row">
            <div class="col-md-2">
                @Html.LabelFor(model => model.PayTypeDescription, htmlAttributes: new { @class = "control-label" })
            </div>
            <div class="col-md-8">
                @Html.EditorFor(model => model.PayTypeDescription, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.PayTypeDescription, "", new { @class = "text-danger" })
            </div>
        </div>
        @Html.Partial("_CreatePageFooterView")
    </div>
}
