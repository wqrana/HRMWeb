@using TimeAide.Services.Helpers;
@model TimeAide.Web.Models.UserInformation

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .radio-toolbar {
        margin: 10px;
    }

        .radio-toolbar input[type="radio"] {
            opacity: 0;
            position: fixed;
            width: 0;
        }

        .radio-toolbar label {
            display: inline-block;
            background-color: #ddd;
            height: 30px;
            width: 30px;
            font-family: sans-serif, Arial;
            /*font-size: 16px;*/
            border: 2px solid #444;
            border-radius: 4px;
            text-align: center;
            padding-top: 2px
        }

            .radio-toolbar label:hover {
                background-color: #dfd;
            }

        .radio-toolbar input[type="radio"]:focus + label {
            border: 2px dashed #444;
        }

        .radio-toolbar input[type="radio"]:checked + label {
            background-color: #bfb;
            border-color: #4c4;
        }

    .profile-info-right table tr {
        height: 30px;
    }

    .title {
        color: #4f4f4f;
    }

    .page-header {
        margin-bottom: 0px;
    }


    .tab-box {
        margin-bottom: 0px;
    }

    .profile-img-wrap.edit-img {
        margin: 0;
        position: absolute;
    }

    textarea {
        width: 100%;
    }

    .table th {
        font-size: small;
        background-color: #d9edf7 !important;
    }

    .table thead th {
        font-size: small;
        background-color: #d9edf7 !important;
    }

    .table tbody td {
        font-size: small;
    }
</style>
<div class="page-header">
    <div class="row">
        <div class="col-sm-12">
            <h3 class="page-title">Profile</h3>
        </div>
    </div>
</div>
<!-- /Page Header -->
@Html.Hidden("userID", @Model.Id)
<div class="alert" style="display:none;" id="userDetailAlert">
    <a href="javascript:void(0)" class="close" id="alertDismiss">&times;</a>
    <strong>Alert!</strong><span class="ml-1"></span>
</div>
<div id="accordion">
    <div class="card mb-0">
        <div class="card-header">
            <div class="text">
                <h3>
                    <a class="card-link" data-toggle="collapse" href="#collapseProfileHeader">
                        <span id="ShortFullNameSpan"> @Html.DisplayFor(model => model.ShortFullName)</span> <span id="showHeader" class="bg-light pull-right fa fa fa-chevron-up" style="font-size:small"></span>
                        @Html.HiddenFor(model => model.ShortFullName)
                    </a>
                </h3>
                <div>
                    Employee ID:<span id="mainEmployeeIdSpan" class="text-muted ml-1">@Html.DisplayFor(model => model.EmployeeId)</span>
                    @{
                        var bgClass = "bg-inverse-danger";
                        if (Model.EmployeeStatusId == 1) //active
                        {
                            bgClass = "bg-inverse-success";
                        }
                        else if (Model.EmployeeStatusId == 2)//inactive
                        {
                            bgClass = "bg-inverse-warning";
                        }
                        else if (Model.EmployeeStatusId == 4)//employee transfer
                        {
                            bgClass = "bg-inverse-info";
                        }
                    }
                    @Html.HiddenFor(model => model.EmployeeStatusId)
                    <span class="badge @bgClass" id="userStatus">@Html.DisplayFor(model => model.EmployeeStatus.EmployeeStatusName)</span>
                </div>
            </div>
        </div>
        <div id="collapseProfileHeader" class="collapse show" data-parent="#accordion">
            <div class="card-body pb-3">
                <div class="row">
                    <div class="col-md-12">
                        <div class="profile-view">
                            <div class="profile-img-wrap edit-img">
                                <!--<img class="inline-block" src="~/Images/no-profile-image.jpg" alt="user" id="profileMainPicture" >-->
                                <img class="inline-block" src="@Model.PictureFilePath" alt="user" id="profileMainPicture">
                                @if (ViewBag.AllowEdit)
                                {
                                    <div class="fileupload btn">
                                        <a href="" class="btn-text" data-toggle="modal" data-target="#upload_picture_modal">Edit</a>
                                    </div>
                                }
                            </div>
                            <div class="profile-basic">
                                <div class="row">
                                    <div class="col-md-5">
                                        <div class="profile-info-left">

                                            <div class="staff-id">Company:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.Company.CompanyName)</span></div>
                                            @{
                                                var activeEmployment = TimeAide.Services.EmploymentService.GetActiveEmployment(Model.Id);
                                                if (activeEmployment != null)
                                                {
                                                    var activeEmploymentHistory = TimeAide.Services.EmploymentHistoryService.GetActiveEmploymentHistory(Model.Id, activeEmployment.Id);
                                                    if (activeEmploymentHistory != null && activeEmploymentHistory.Location != null)
                                                    {
                                                        <div class="staff-id">Location:<span class="text-muted ml-1">@activeEmploymentHistory.Location.LocationName</span></div>
                                                    }
                                                    else
                                                    {
                                                        <div class="staff-id">Location:<span class="text-muted ml-1"></span></div>
                                                    }
                                                }
                                                else
                                                {
                                                    <div class="staff-id">Location:<span class="text-muted ml-1"></span></div>
                                                }
                                            }
                                            <div class="staff-id">Department:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.Department.DepartmentName)</span></div>
                                            <div class="staff-id">Sub-Department:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.SubDepartment.SubDepartmentName)</span></div>
                                        </div>
                                    </div>
                                    <div class="col-md-7">
                                        <div class="profile-info-right">
                                            <div class="staff-id">Employee Type:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.EmployeeType.EmployeeTypeName)</span></div>
                                            <div class="staff-id">Position:<span class="text-muted ml-1">@Html.DisplayFor(model => model.Position.PositionName) </span></div>
                                            <div class="staff-id">Employment Status:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.EmploymentStatu.EmploymentStatusName)</span></div>
                                            @*<div class="staff-id">Supervisory Level:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.SupervisoryLevel.SupervisoryLevelName)</span></div>*@
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
