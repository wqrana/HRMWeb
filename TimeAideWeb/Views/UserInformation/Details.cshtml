<div class="tab-content pt-0">
    <div id="emp_generalInfo" class="pro-overview tab-pane fade show active">
        <div class="row">
            <div class="col-md-12 d-flex">
                <div class="card profile-box flex-fill">
                    <div class="card-body pt-1">
                        <!--<h3 class="card-title">General Information  <a href="javascript:editGeneralInfo()" class="edit-icon" title="Edit General Info."><i class="fa fa-pencil"></i></a></h3>-->
                        <!--Tab Title & Right dropdown menu-->
                        <div class="row">
                            <div class="col-md-6">
                                <h3 class="card-title pull-left">
                                    General Information
                                </h3>
                            </div>
                            <div class="col-md-6 pr-0">
                                <div class="row">
                                    <div class="col-md-11 pl-0 pr-0">
                                        <div class="nav btn-toolbar pull-right bg-gradient-blue">
                                            <div class="btn-group" role="group">
                                                <a class="nav-link btn btn-primary" role="button" href="javascript:openAdhocReport(0,@Model.EmployeeId)">Print</a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="bg-gradient-blue inline-block">
                                        @if (!SecurityHelper.IsUser)
                                        {
                                            <div class="pull-right dropdown-action">

                                                <a href="#" title="Action Menu" class="action-icon dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="material-icons pt-2" style="color:white">more_vert</i></a>
                                                <div class="dropdown-menu dropdown-menu-right" x-placement="bottom-end" style="position: absolute; will-change: transform; top: 0px; left: 0px; transform: translate3d(25px, 20px, 0px);">
                                                    @if (ViewBag.AllowEdit)
                                                    {
                                                        <a href="javascript:editGeneralInfo()" class="dropdown-item"><i class="fa fa-pencil m-r-5"></i> Edit</a>
                                                        <a href="javascript:activateUser()" class="dropdown-item"><i class="fa fa-user-plus m-r-5"></i> Activate</a>
                                                        <a href="javascript:deactivateUser()" class="dropdown-item"><i class="fa fa-user-times m-r-5"></i> Deactivate</a>
                                                        <a href="javascript:transferUser()" class="dropdown-item"><i class="fa fa-chevron-up m-r-5"></i> Employee Transfer</a>
                                                        <a href="" data-toggle="modal" data-target="#upload_userCV_modal" class="dropdown-item"><i class="fa fa-upload m-r-5"></i> Upload CV</a>
                                                    }
                                                    <a href="javascript:downloadUserCV()" class="dropdown-item"><i class="fa fa-download m-r-5"></i> Download CV</a>
                                                    @*@if (SecurityHelper.AllowChangeHistory("UserInformation"))*@
                                                    @if (ViewBag.AllowChangeHistory)
                                                    {
                                                        {
                                                            <a href="javascript:void(0);" id="anchorChangeHistory" onclick="ShowChangeHistory(@Model.Id,'UserInformation')" class="dropdown-item" data-id="@Model.Id"><i class="fa fa-pencil m-r-5"></i> Change History</a>
                                                            @*<a href="javascript:void(0);" class="anchorEdit" data-id="@Model.Id">Edit</a>*@
                                                        }
                                                    }
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">

                            <div class="row">
                                <div class="panel panel-default" style="width:100%;margin-left:10px">
                                    <div class="panel-heading" style="padding-left:10px">General Info.</div>
                                    <div class="panel-body">
                                        <div class="col-md-12">

                                            <div class="form-group row">

                                                <label class="col-md-2 col-form-label">Employee Id:</label>
                                                <div class="col-md-2">
                                                    <div id="employeeIdDiv" class="text-muted mt-2">@Html.DisplayFor(m => m.EmployeeId)</div>
                                                </div>
                                                <label class="col-md-2 col-form-label">SSN:</label>
                                                <div class="col-md-2">
                                                    <div class="text-muted mt-2">
                                                        @*<span id="SSNDiv">@Html.DisplayFor(m => m.SSNComputed)</span>*@
                                                        <span id="SSNDiv">
                                                            @TimeAide.Services.UserInformationService.MaskSsn(Model.SSNEncrypted)

                                                        </span>
                                                        <input type="hidden" id="hdnSSNDe" value="@Model.SSN" />
                                                        <input type="hidden" id="hdnSSNMasked" value="@Model.SSNComputed" />
                                                    </div>
                                                </div>
                                                <label class="col-md-2 col-form-label">First Name:</label>
                                                <div class="col-md-2">
                                                    <div id="firstNameDiv" class="text-muted mt-2">@Html.DisplayFor(m => m.FirstName)</div>
                                                </div>
                                            </div>

                                            <div class="form-group row">

                                                <label class="col-md-2 col-form-label">Initial:</label>
                                                <div class="col-md-2">
                                                    <div id="middleInitialDiv" class="text-muted mt-2">@Html.DisplayFor(m => m.MiddleInitial)</div>
                                                </div>
                                                <label class="col-md-2 col-form-label">1st LastName:</label>
                                                <div class="col-md-2">
                                                    <div id="firstLastNameDiv" class="text-muted mt-2">@Html.DisplayFor(m => m.FirstLastName)</div>
                                                </div>
                                                <label class="col-md-2 col-form-label">2nd LastName:</label>
                                                <div class="col-md-2">
                                                    <div id="secondLastNameDiv" class="text-muted mt-2">@Html.DisplayFor(m => m.SecondLastName)</div>
                                                </div>

                                            </div>
                                            <div class="form-group row">

                                                <label class="col-md-2 col-form-label">Birth Date:</label>
                                                <div class="col-md-2">
                                                    <div id="dobDiv" class="text-muted mt-2">@Html.DisplayFor(m => m.BirthDate)</div>
                                                </div>
                                                <label class="col-md-2 col-form-label">Birth Place:</label>
                                                <div class="col-md-2">
                                                    <div id="BirthPlaceDiv" class="text-muted mt-2">
                                                        @Html.DisplayFor(m => m.BirthPlace)
                                                    </div>
                                                </div>
                                                <label class="col-md-2 col-form-label">Gender:</label>
                                                <div class="col-md-2">
                                                    @{
                                                        var genderList = ViewBag.GenderId as SelectList;
                                                        var genderid = Model.GenderId;
                                                        var genderText = genderList.Where(w => w.Value == genderid.ToString()).Select(s => s.Text).FirstOrDefault();
                                                    }
                                                    <div id="genderTextDiv" class="text-muted mt-2">@genderText</div>
                                                    @Html.Hidden("genderIdDiv", @Model.GenderId)
                                                </div>

                                            </div>
                                            <div class="form-group row">

                                                <label class="col-md-2 col-form-label">Marital Status:</label>
                                                <div class="col-md-2">
                                                    @{
                                                        var maritalList = ViewBag.MaritalStatusId as SelectList;
                                                        var maritalid = Model.MaritalStatusId;
                                                        var maritalStatusText = maritalList.Where(w => w.Value == maritalid.ToString()).Select(s => s.Text).FirstOrDefault();
                                                    }
                                                    <div id="martialStatusTextDiv" class="text-muted mt-2">@maritalStatusText</div>
                                                    @Html.Hidden("maritalStatusIdDiv", @Model.MaritalStatusId)
                                                </div>
                                                <label class="col-md-2 col-form-label">Ethnicity:</label>
                                                @{
                                                    var ethnicityId = Model.EthnicityId;
                                                    var ethnicityText = ethnicityId != null ? Model.Ethnicity.EthnicityName : "";
                                                    var disabilityId = Model.DisabilityId;
                                                    var disabilityText = disabilityId != null ? Model.Disability.DisabilityName : ""; ;

                                                }
                                                <div class="col-md-2">
                                                    <div id="ethnicityTextDiv" class="text-muted mt-2">@ethnicityText</div>
                                                    @Html.Hidden("ethnicityIdDiv", @ethnicityId)
                                                </div>
                                                <label class="col-md-2 col-form-label">Disability:</label>
                                                <div class="col-md-2">
                                                    <div id="disabilityTextDiv" class="text-muted mt-2">@disabilityText</div>
                                                    @Html.Hidden("disabilityIdDiv", @disabilityId)
                                                </div>
                                            </div>
                                            <div class="form-group row">

                                                <label class="col-md-2 col-form-label">CV Name:</label>
                                                <div class="col-md-2">
                                                    <div id="CVNameDiv" class="text-muted mt-2">@Model.ResumeFilePath</div>
                                                </div>
                                                @{
                                                    var selectedList = Model.EmployeeVeteranStatus.Select(s => s.VeteranStatusId.ToString()).ToList();
                                                    var VeteranStatusText = string.Join(",", ((SelectList)ViewBag.VeteranStatusId).Where(w => selectedList.Contains(w.Value)).Select(s => s.Text));
                                                    var selectedListId = string.Join(",", ((SelectList)ViewBag.VeteranStatusId).Where(w => selectedList.Contains(w.Value)).Select(s => s.Value));
                                                }
                                                <label class="col-md-2 col-form-label">Veteran Status:</label>
                                                <div class="col-md-6">
                                                    <div id="VeteranStatusTextDiv" class="text-muted mt-2">@VeteranStatusText</div>
                                                    @Html.Hidden("VeteranStatusIdDiv", @selectedListId)
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div> <!-- Main Gen Row-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--General Info End-->
