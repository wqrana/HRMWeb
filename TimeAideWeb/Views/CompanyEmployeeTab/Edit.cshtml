@model TimeAide.Web.Models.CompanyEmployeeTab
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<div class="form-horizontal" style="padding:10px">
    @Html.Partial("_CreatePageTitleView")
    @Html.HiddenFor(model => model.Id)

    <div class="form-group row">
        <div class="col-md-3">
            @Html.LabelFor(model => model.Form.FormName, htmlAttributes: new { @class = "control-label" })
        </div>
        <div class="col-md-7">
            @Html.EditorFor(model => model.Form.FormName, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.Form.FormName, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            @Html.LabelFor(model => model.CompanyId, "Company", htmlAttributes: new { @class = "control-label" })
        </div>
        <div class="col-md-7">
            @(Html.CustomDropDownList<TimeAide.Web.Models.Company>("CompanyId", null, Model.CompanyId, htmlAttributes: new { @class = "form-control" }))
            @Html.ValidationMessageFor(model => model.CompanyId, "", new { @class = "text-danger" })
        </div>
    </div>
    @Html.Partial("_CreatePageFooterView")
</div>
}

