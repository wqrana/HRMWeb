@model IEnumerable<TimeAide.Web.Models.EmployeeNotificationRole>

<div class="form-group row">
    <div class="col-md-12">
        <table class="table table-striped custom-table table-nowrap mb-0" id="tblUser">
            <thead>
                <tr>
                    <th>
                        Notification Role Id
                    </th>
                    <th>
                        Employee Group Name
                    </th>
                    <th>
                        Employee Group Name
                    </th>
                </tr>
            </thead>
            <tbody>
                @if (Model != null)
                {
                    foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Id)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.EmployeeGroup.EmployeeGroupName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.EmployeeGroup.EmployeeGroupName)
                            </td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>
