@model TimeAide.Web.Models.UserContactInformation
<div style="font-size: 90%;">
    <div class="form-group row">
        <div class="col-md-6  p-0">
            <!--<div class="panel panel-default" style="width:49%;margin-left:10px">-->
            <div class="panel panel-default" style="margin-left:5px">
                <div class="panel-heading" style="padding-left:10px">
                    <div class="form-group row">
                        <div class="col-md-6">
                            Telephone Numbers
                        </div>
                        <div class="col-md-6">
                            @if (ViewBag.AllowAdd || ViewBag.AllowEdit)
                            {
                                <div class="pull-right dropdown-action">
                                    <a href="#" class="action-icon dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="material-icons">more_horiz</i></a>
                                    <div class="dropdown-menu dropdown-menu-right" x-placement="bottom-end" style="position: absolute; will-change: transform; top: 0px; left: 0px; transform: translate3d(38px, 32px, 0px);">
                                        <a a href="javascript:void(0);" id="anchorUserContactInformation" onclick="fnAddEditUserContactInformation(@Model.UserInformationId,'PhoneNumbers');" class="dropdown-item" data-id="@Model.Id"><i class="fa fa-pencil m-r-5"> Edit</i></a>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
                <div class="panel-body">
                    <div class="form-group row">
                        <label class="col-md-2 col-form-label" style="margin-left:5px">
                            Home:
                        </label>
                        <div class="col-md-3 text-muted mt-2">
                            @if (String.IsNullOrEmpty(Model.HomeNumber) || Model.HomeNumber.Trim().Length < 10)
                            {
                                @Model.HomeNumber
                            }
                            else
                            {
                                @String.Format("({0}) {1}-{2}", Model.HomeNumber.Substring(0, 3), Model.HomeNumber.Substring(3, 3), Model.HomeNumber.Substring(6, 4))
                            }
                        </div>

                        <label class="col-md-2 col-form-label" style="margin-left:5px">
                            Mobile:
                        </label>
                        <div class="col-md-3 text-muted mt-2">
                            @if (String.IsNullOrEmpty(Model.CelNumber) || Model.CelNumber.Trim().Length < 10)
                            {
                                @Model.CelNumber
                            }
                            else
                            {
                                @String.Format("({0}) {1}-{2}", Model.CelNumber.Substring(0, 3), Model.CelNumber.Substring(3, 3), Model.CelNumber.Substring(6, 4))
                            }
                        </div>

                    </div>
                    <div class="form-group row">
                        <label class="col-md-2 col-form-label" style="margin-left:5px">
                            Fax:
                        </label>
                        <div class="col-md-3 text-muted mt-2">
                            @if (String.IsNullOrEmpty(Model.FaxNumber) || Model.FaxNumber.Trim().Length < 10)
                            {
                                @Model.FaxNumber
                            }
                            else
                            {
                                @String.Format("({0}) {1}-{2}", Model.FaxNumber.Substring(0, 3), Model.FaxNumber.Substring(3, 3), Model.FaxNumber.Substring(6, 4))
                            }
                        </div>

                        <label class="col-md-2 col-form-label" style="margin-left:5px">
                            Other:
                        </label>
                        <div class="col-md-3 text-muted mt-2">
                            @if (String.IsNullOrEmpty(Model.OtherNumber) || Model.OtherNumber.Trim().Length < 10)
                            {
                                @Model.OtherNumber
                            }
                            else
                            {
                                @String.Format("({0}) {1}-{2}", Model.OtherNumber.Substring(0, 3), Model.OtherNumber.Substring(3, 3), Model.OtherNumber.Substring(6, 4))
                            }
                        </div>

                    </div>
                    <div class="form-group row">
                        <label class="col-md-2 col-form-label" style="margin-left:5px">
                            Work:
                        </label>
                        <div class="col-md-3 text-muted mt-2">
                            @if (String.IsNullOrEmpty(Model.WorkNumber) || Model.WorkNumber.Trim().Length < 10)
                            {
                                @Model.WorkNumber
                            }
                            else
                            {
                                @String.Format("({0}) {1}-{2}", Model.WorkNumber.Substring(0, 3), Model.WorkNumber.Substring(3, 3), Model.WorkNumber.Substring(6, 4))
                            }
                        </div>

                        <label class="col-md-3 col-form-label" style="margin-left:5px">
                            Work Extension:
                        </label>
                        <div class="col-md-3 text-muted mt-2">
                            <span id="lblWorkExtension"> @Html.DisplayFor(model => model.WorkExtension)</span>
                        </div>

                    </div>
                    <div class="form-group row">
                    </div>
                    <div class="form-group row">
                    </div>
                    <div class="form-group row">
                    </div>
                </div>
        </div>
       </div>
        <div class="col-md-6  p-0">
