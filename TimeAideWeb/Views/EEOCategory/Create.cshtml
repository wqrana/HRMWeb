@model TimeAide.Web.Models.EEOCategory
@using (Html.BeginForm())
{
    <div class="form-horizontal" style="padding:10px">
        @Html.AntiForgeryToken()
        @Html.Partial("_CreatePageTitleView")

        <div class="form-group row">
            <div class="col-md-3">
                @Html.LabelFor(model => model.EEONumber, htmlAttributes: new { @class = "control-label" })
            </div>
            <div class="col-md-8">
                @Html.EditorFor(model => model.EEONumber, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.EEONumber, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group row">
            <div class="col-md-3">
                @Html.LabelFor(model => model.EEOCategoryName, htmlAttributes: new { @class = "control-label" })
            </div>
            <div class="col-md-8">
                @Html.EditorFor(model => model.EEOCategoryName, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.EEOCategoryName, "", new { @class = "text-danger" })
            </div>
        </div>
        @Html.Partial("_CreatePageFooterView")
    </div>
}
