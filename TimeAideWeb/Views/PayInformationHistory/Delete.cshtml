                                <span id="lblTerminationEligibility"> @Html.DisplayFor(model => model.ApprovedDate)</span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-md-3 col-form-label" style="margin-left:10px">
                                Approved By
                            </label>
                            <div class="col-lg text-muted mt-2">
                                <span id="lblApprovedBy"> @Html.DisplayFor(model => model.SelectedAuthorizByName)</span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-md-3 col-form-label" style="margin-left:10px">
                                Change Reason
                            </label>
                            <div class="col-lg text-muted mt-2">
                                <span id="lblTerminationNotes"> @Html.DisplayFor(model => model.ChangeReason)</span>
                            </div>
                        </div>
                    </div>
                </div>*@
        </div>

    </div>
</div>


<div class="submit-section" style="text-align:center">
    <button class="btn btn-danger submit-btn m-r-10" id="btnDeletePayInformationHistory">Delete</button>
    <button type="submit" class="btn btn-danger submit-btn m-r-10" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span> Cancel</button>
</div>
