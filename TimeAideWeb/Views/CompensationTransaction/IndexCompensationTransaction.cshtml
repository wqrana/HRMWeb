@model IEnumerable<TimeAide.Web.Models.CompensationTransaction>
<div class="form-group row">
    <div class="col-md-12">
        <table class="table table-striped custom-table table-nowrap mb-0" id="tblUser">
            <thead>
                <tr>
                    <th>
                        Name
                    </th>
                    <th>
                        Description
                    </th>
                    <th>
                        Primary Transaction
                    </th>
                </tr>
            </thead>
            @if (Model != null)
            {
                foreach (var item in Model)
                {
                    <tr>

                        <td>
                            @Html.DisplayFor(modelItem => item.TransactionConfiguration.ConfigurationName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TransactionConfiguration.ConfigurationDescription)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TransactionConfiguration.PrimaryTransaction.PrimaryTransactionName)
                        </td>
                    </tr>
                }
            }
        </table>
    </div>
</div>