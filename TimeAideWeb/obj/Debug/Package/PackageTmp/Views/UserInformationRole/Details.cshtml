@model TimeAide.Web.Models.UserInformationRole
<div style="padding:10px;padding-bottom:0px">
    @Html.Partial("_CreatePageTitleView")
    <dl class="dl-horizontal">
        <dt>
            @Html.Label("Role")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Role.RoleName)
        </dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>
            @Html.Label("User")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserInformation.FullName)
        </dd>
    </dl>
</div>
<p>
    <button type="submit" class="btn btn-primary submit-btn" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span> Cancel</button>
</p>
