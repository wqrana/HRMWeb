                                    <a href="#"><span>@eachClient.ClientName</span> <span class="menu-arrow"></span></a>
                                    <ul style="display: none;">
                                        @foreach (TimeAide.Web.Models.Company eachCompany in TimeAide.Services.CompanyService.GetCompany(eachClient.Id).OrderBy(o => o.CompanyName))
                                        {
                                            <li class="submenu">
                                                <a href="javascript:void(0);" onclick="SelectCompany(@eachCompany.Id)"><span> @eachCompany.CompanyName </span> </a>
                                            </li>
                                        }
                                    </ul>
                                }
                            </li>
