@model IEnumerable<TimeAide.Web.Models.IncidentTreatmentFacility>
@Html.Partial("_IndexPageHeaderView")
<thead>
    <tr>
        <th>Action</th>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TreatmentFacilityName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TreatmentFacilityAddress)
        </th>
        <th>
            State
        </th>
        <th>
            City
        </th>
        <th>
            Zip
        </th>

    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
    <tr>
        <td width="7%">
            @Html.Partial("_IndexPageButtonsView", item)
        </td>
        <td>

            <a href="javascript:void(0);" id="anchorDetail" data-id="@item.Id">@item.Id</a>

        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TreatmentFacilityName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TreatmentFacilityAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State.StateName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City.CityName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ZipCode)
        </td>

    </tr>
    }
</tbody>
@Html.Partial("_IndexPageFooterView")
