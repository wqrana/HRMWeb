@model TimeAide.Web.Models.RateFrequency
<div class="form-horizontal" style="padding:10px">
    @Html.Partial("_CreatePageTitleView")
    @Html.HiddenFor(model => model.Id)

    <div class="form-group row">
        <div class="col-md-4">
            @Html.LabelFor(model => model.RateFrequencyName, htmlAttributes: new { @class = "control-label" })
        </div>
        <div class="col-md-8">
            @Html.EditorFor(model => model.RateFrequencyName, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.RateFrequencyName, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group row">
        <div class="col-md-4">
            @Html.LabelFor(model => model.RateFrequencyDescription, htmlAttributes: new { @class = "control-label" })
        </div>
        <div class="col-md-8">
            @Html.EditorFor(model => model.RateFrequencyDescription, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.RateFrequencyDescription, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group row">
        <div class="col-md-4">
            @Html.LabelFor(model => model.HourlyMultiplier, htmlAttributes: new { @class = "control-label" })
        </div>
        <div class="col-md-8">
            @Html.EditorFor(model => model.HourlyMultiplier, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.HourlyMultiplier, "", new { @class = "text-danger" })
        </div>
    </div>

    @Html.Partial("_CreatePageFooterView")
</div>
