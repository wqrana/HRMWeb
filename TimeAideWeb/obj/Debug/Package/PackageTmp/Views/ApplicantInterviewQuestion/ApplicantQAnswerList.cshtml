@model IEnumerable<TimeAide.Web.Models.ApplicantQAnswerOption>
<div class="table-responsive">
    <table class="table table-striped table-condensed table-sm table-nowrap mb-0" id="">

        <thead>
            <tr>
               
                <th width="10%">Action</th>
                <th width="10%">Sr.</th>
                <th>@Html.DisplayNameFor(m=>m.AnswerOptionName)</th>
                
            </tr>
        </thead>
        @{ var srNo = 0;}
        @foreach (var item in Model)
        {
            ++srNo;
        <tr>
            <td class="text-left">
                <div class="dropdown dropdown-action">
                    <a href="#" class="action-icon dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="material-icons">more_vert</i></a>
                    <div class="dropdown-menu dropdown-menu-right" x-placement="bottom-end" style="position: absolute; will-change: transform; top: 0px; left: 0px; transform: translate3d(38px, 32px, 0px);">
                        <a class="dropdown-item" href="javascript:createEditAnswerOption(@item.Id)"><i class="fa fa-pencil m-r-5"></i> Edit</a>

                        <a class="dropdown-item" href="javascript:getAnswerOptionDeleteData(@item.Id)"><i class="fa fa-trash-o m-r-5"></i> Delete</a>

                    </div>
                </div>
            </td>
            <td>@srNo</td>
            <td>@Html.DisplayFor(m=>item.AnswerOptionName)</td>


        </tr>
        }

    </table>
</div>
