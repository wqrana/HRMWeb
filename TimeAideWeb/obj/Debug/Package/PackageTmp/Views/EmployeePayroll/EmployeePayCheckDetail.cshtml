    function getEmployeePayCheckEditData(id) {
        // $("#divLoadingLicensesTab").show();
        $.ajax({
            type: "get",
            url: '/EmployeePayroll/EmployeePayCheckEdit',
            data: { "id": id },

            success: function (data) {
                //debugger;
                //console.log(data);
                $("#divPayCheckEdit").html(data);
                $("#employeePayCheckEdit_modal").modal("show");
