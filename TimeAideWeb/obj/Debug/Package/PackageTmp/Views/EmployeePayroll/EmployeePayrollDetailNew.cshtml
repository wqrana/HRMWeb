<script>
    $(document).ready(function () {


    });
    function getEmployeePayCheckDetail(id) {
        // $("#divLoadingLicensesTab").show();
        $.ajax({
            type: "get",
            url: '/EmployeePayroll/EmployeePayCheckDetail',
            data: { "id": id },

            success: function (data) {
                //debugger;
                //console.log(data);
                $("#divPayrollEditPopup").html(data);
                $("#employeePayCheckDetail_modal").modal("show");
            },
            error: function () {
                showAlertAutoHide("", "Error", data.ErrorMessage);
            }
        });
    }
    function getEmployeeCompensationDetail(id) {
        // $("#divLoadingLicensesTab").show();
        $.ajax({
            type: "get",
            url: '/EmployeePayroll/EmployeeCompensationDetail',
            data: { "id": id },

            success: function (data) {
                //debugger;
                //console.log(data);
                $("#divPayrollEditPopup").html(data);
                $("#employeeCompensationDetail_modal").modal("show");
            },
            error: function () {
                showAlertAutoHide("", "Error", data.ErrorMessage);
            }
        });
    }

</script>

