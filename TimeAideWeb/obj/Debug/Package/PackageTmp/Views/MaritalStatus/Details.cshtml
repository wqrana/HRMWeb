@model TimeAide.Web.Models.MaritalStatus
<div style="padding:10px">
    @Html.Partial("_CreatePageTitleView")
    <dl class="dl-horizontal">
        <dt>
            Marital Status
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MaritalStatusName)
        </dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>
            Description
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MaritalStatusDescription)
