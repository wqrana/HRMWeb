@model TimeAide.Web.Models.UserInformation

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .profile-info-right table tr {
        height: 30px;
    }

    .title {
        color: #4f4f4f;
    }
</style>
<div class="page-header">
    <div class="row align-items-center">
        <div class="col-sm-12">
            <h3 class="page-title">Delete</h3>
            <ul class="breadcrumb">
                <li class="breadcrumb-item">User</li>
                <li class="breadcrumb-item active">Delete</li>

            </ul>
        </div>
    </div>
</div>
<div class="alert alert-info alert-dismissible fade show" role="alert">
    <strong>Alert!</strong> Are you sure you want to delete this?
</div>

    <div class="card mb-0">
        <div class="card-body">
            <div class="row">
                <div class="col-md-12">
                    <div class="profile-view">
                        <div class="profile-img-wrap">
                            <div class="profile-img">
                                <!--<a href="#"><img alt="" src="/Content/Themes/assets/img/profiles/avatar-02.jpg"></a>-->
                                <a href="#"><img alt="" src="~/Images/user-profile-pic.jpg"></a>
                            </div>
                        </div>
                        <div class="profile-basic">
                            <div class="row">
                                <div class="col-md-5">
                                    <div class="profile-info-left">
                                        <h3 class="user-name m-t-0 mb-0"> @Html.DisplayFor(model => model.ShortFullName)</h3>

                                        <div class="staff-id">Employee ID:<span class="text-muted ml-1">@Html.DisplayFor(model => model.Id)</span></div>
                                        <div class="staff-id">System ID:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.SystemId) </span> </div>
                                        <!--<div class="staff-id">Job Code:<span class="text-muted ml-1">  @Html.DisplayFor(model => model.DefaultJobCode.JobCodeName)</span></div>-->
                                        <div class="staff-id">Employee Type:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.EmployeeType.EmployeeTypeName)</span></div>
                                        <!--<div class="staff-id">Supervisory Level:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.SupervisoryLevel.SupervisoryLevelName)</span></div>-->
                                            <div class="staff-id">Employment Status:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.EmploymentStatu.EmploymentStatusName)</span></div>
                                            <div class="staff-id">Client:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.Client.ClientName)</span></div>
                                            <div class="staff-id">Company:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.Company.CompanyName)</span></div>
                                            <div class="staff-id">Department:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.Department.DepartmentName)</span></div>
                                            <div class="staff-id">Sub-Department:<span class="text-muted ml-1"> @Html.DisplayFor(model => model.SubDepartment.SubDepartmentName)</span></div>

                                        </div>
                                    </div>
                                    <div class="col-md-7">
                                        <div class="profile-info-right">
                                            <table>
                                                @*
                                                <tr>
                                                    <td class="title">Login Email:</td>
                                                   <td class="text-muted ml-4">@Html.DisplayFor(model => model.AspNetUsers.Email)</td>
                                                </tr>*@
                                                <tr>
                                                    <td class="title">First Name:</td>
                                                    <td class="text-muted ml-4">@Html.DisplayFor(model => model.FirstName)</td>
                                                </tr>
                                                <tr>
                                                    <td class="title">Middle:</td>
                                                    <td class="text-muted ml-4">@Html.DisplayFor(model => model.MiddleInitial)</td>
                                                </tr>
                                                <tr>
                                                    <td class="title">1st Last Name:</td>
                                                    <td class="text-muted">@Html.DisplayFor(model => model.FirstLastName)</td>
                                                </tr>
                                                <tr>
                                                    <td class="title">2nd Last Name:</td>
                                                    <td class="text-muted">@Html.DisplayFor(model => model.SecondLastName)</td>
                                                </tr>
                                                <tr>
                                                    <td class="title">DOB:</td>
                                                    <td class="text-muted">@Html.DisplayFor(model => model.BirthDate)</td>
                                                </tr>


                                                <tr>
                                                    <td class="title">SSN End:</td>
                                                    <td class="text-muted">@Html.DisplayFor(model => model.SSNEnd)</td>

                                                </tr>
                                            </table>
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="submit-section">
                        <button class="btn btn-danger submit-btn m-r-10">Delete</button>
                    </div>
                }
            </div>
        </div>
        <div class="mt-4">

            <a href="/UserInformation/Index"><i class="fa fa-arrow-left"></i> Back to List</a>

        </div>

