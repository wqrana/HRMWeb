@model IEnumerable<TimeAide.Web.Models.EmployeeNotification>

@Html.Partial("_IndexPageHeaderView")
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeNotificationName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeNotificationDescription)
        </th>
        <th>
            Employee Notification Type
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsAllCompanies)
        </th>
        <th></th>
    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmployeeNotificationName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmployeeNotificationDescription)
        </td>
        <td>
            @Html.DisplayFor(model => item.EmployeeNotificationType.EmployeeNotificationTypeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsAllCompanies)
        </td>
        <td>
            @Html.Partial("_IndexPageButtonsView", item)
        </td>
    </tr>
    }
</tbody>
@Html.Partial("_IndexPageFooterView")
