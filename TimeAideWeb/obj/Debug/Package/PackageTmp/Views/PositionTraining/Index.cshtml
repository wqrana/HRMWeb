@model IEnumerable<TimeAide.Web.Models.PositionTraining>
<div class="table-responsive">
    <table class="table table-striped custom-table table-nowrap mb-0">
        <tr>
            <th>
                Training Name
            </th>

        </tr>
        @if (Model!= null)
        {
            foreach (var item in Model)
            {
                <tr>

                    <td>@item.Training.TrainingName</td>

                </tr>
            }
        }
    </table>
</div>
