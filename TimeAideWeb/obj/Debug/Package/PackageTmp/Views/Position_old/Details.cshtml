@model TimeAide.Web.Models.Position
<div style="padding:10px">
    @Html.Partial("_CreatePageTitleView")
    <dl class="dl-horizontal">
        <dt>
            Position
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PositionName)
        </dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>
            Description
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PositionDescription)
        </dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>
            Position Code
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PositionCode)
        </dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>
            Default Pay Scale
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DefaultPayScale.PayScaleName)
        </dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>
            Default EEO Category
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DefaultEEOCategory.EEOCategoryName)
        </dd>

    </dl>
</div>
<p>
    <button type="submit" class="btn btn-primary submit-btn" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span> Cancel</button>
</p>
