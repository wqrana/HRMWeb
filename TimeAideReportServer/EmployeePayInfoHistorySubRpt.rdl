      <Query>
        <DataSourceName>TimeAideLocalDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@userInfoId">
            <Value>=Parameters!userInfoId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@currentEmploymentId">
            <Value>=Parameters!currentEmploymentId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@currentPayInfoHistoryId">
            <Value>=Parameters!currentPayInfoHistoryId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select 
payInfo.UserInformationId
,payInfo.PayInformationHistoryId
,payInfo.EmploymentId
,payInfo.RateAmount
,payInfo.YearlyGrossPay
,payInfo.PeriodGrossPay
,payInfo.RateFrequencyName
,payInfo.PayTypeName
,payInfo.PayFrequencyName
,payInfo.EEOCategoryName
,payInfo.PayInfoEndDate
,payInfo.StartDate as PayInfoStartDate
, ChangeReason
From vw_rpt_EmployeePayInfoHistory payInfo
Where UserInformationId =@userInfoId
And  EmploymentId = @currentEmploymentId
And PayInformationHistoryId&lt;&gt; @currentPayInfoHistoryId</CommandText>
      </Query>
